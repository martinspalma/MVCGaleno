@model IEnumerable<MVCGaleno.Models.Cita>

@{
    ViewData["Title"] = "Index";
}

<h1>Citas</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.fechaCita)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.estaDisponible)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PrestadorMedico)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.fechaCita)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.estaDisponible)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.PrestadorMedico.IdPrestador)
                </td>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.IdCita">Edit</a> |
                    <a asp-action="Details" asp-route-id="@item.IdCita">Details</a> |
                    <a asp-action="Delete" asp-route-id="@item.IdCita">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
